pipeline{
    agent any
    tools {
        jfrog 'jfrog-cli'
    }
    stages {
        stage ('Testing') {
            steps {
                jf '-v' 
                jf 'c show'
                jf 'rt ping'
                sh 'touch test-file'
                jf 'rt u test-file jfrog-cli-2/'
                jf 'rt bp'
                jf 'rt dl jfrog-cli-2/test-file'
            }
        } 
    }
}
